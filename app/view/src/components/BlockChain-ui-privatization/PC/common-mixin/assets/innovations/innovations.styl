.page-innovations {
  .flowingWater-nav {
    height: 120px;
    line-height: 55px;
    font-size: 32px;
    display: flex;
    align-items: center;
  }
  .flowingWater-options {
    width: 100%;
    box-sizing: border-box;
    padding: 0 20px;
    margin-top: 14px;
    .common-select {
      display: inline-block;
      margin-right: 30px;
    }
  }
  .flowingWater-table {
    width: 100%;
    box-sizing: border-box;
    margin-top: 35px;
    padding: 0 44px;
    .table-head, .table-body-bar, .pagination{
      padding: 0!important;
    }
  }
}