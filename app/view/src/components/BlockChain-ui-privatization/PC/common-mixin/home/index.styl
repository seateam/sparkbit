.home-container {
    //color: var(--color-text-primary);
    //background: var(--color-BG);
    color: #ffffff;
    background: #000000;
    .home-p1 {
        padding: 100px 100px 60px;
        text-align: center;
        ul {
            display: flex;
            justify-content: center;
            padding-block: 28px 40px;
            margin-top: 32px;
            gap: 28px;
            li {
                display: flex;
                flex-direction: column;
                align-items: flex-ends;
                width: 254px;
                height: 350px;
                padding-block: 24px 32px;
                cursor: pointer;
                will-change: auto;
                border-radius: 20px;
                transition: all .3s ease;
                border: 2px solid transparent;
                box-sizing: border-box;
                gap: 16px;
                .logos {
                    width: 169px;
                    height: 218px;
                    margin-inline: auto;
                    .active {
                        display: none;
                    }
                }
                img {
                    width: 100%;
                    height: 100%;
                }
                .item-name {
                    font-size: 26px;
                    line-height: 34px;
                    font-weight: 700;
                    margin-bottom: 8px;
                }
                .item-desc {
                    font-size: 15px;
                    line-height: 18px;
                    font-weight: 500;
                    opacity: .6;
                    letter-spacing: -.04em;
                }
                &:hover {
                    transition-delay: 0s !important;
                    transform: scale(1.05) !important;
                    border-color: var(--color-BTN-primary);
                    .normal {
                        display: none;
                    }
                    .active {
                        display: block;
                    }
                }
            }
        }
    }
    .home-p2 {
        padding: 100px 40px;
        .top {
            display: flex;
            &>div {
                flex: 1;
            }
            .subtitle {
                max-width: 600px;
            }
        }
        ul {
            margin-top: 60px;
            display: flex;
            justify-content: center;
            gap: 40px;
            li {
                width: 340px;
                min-height: 380px;
                padding: 40px 30px 20px;
                border: 1px solid #7D7D7D;
                border-radius: 20px;
                box-sizing: border-box;
                .logo {
                    width: 150px;
                    height: 180px;
                    margin-bottom: 20px;
                }
                img {
                    width: 100%;
                    height: 100%;
                }
                .item-name {
                    font-size: 28px;
                    line-height: 32px;
                    font-weight: 700;
                    margin-bottom: 16px;
                }
                .item-desc {
                    font-size: 16px;
                    line-height: 22px;
                    font-weight: 500;
                    letter-spacing: -.04em;
                    opacity: .6;
                }
            }
        }
    }
    .home-p3 {
        padding: 100px 40px 60px;
        text-align: center;
        h3, .subtitle {
            margin-inline: auto;
        }
        h3 {
            max-width: 700px;
        }
        .subtitle {
            max-width: 600px;
        }
        .video-wrap {
            width: 880px;
            height: auto;
            margin-inline: auto;
            video {
                width: 100%;
                height: auto;
            }
        }
        ul {
            padding-block: 40px;
            display: flex;
            justify-content: center;
            text-align: left;
            gap: 80px;
            li {
                position: relative;
                flex: 1;
                .item-name {
                    font-size: 28px;
                    line-height: 32px;
                    font-weight: 700;
                    margin-bottom: 12px;
                }
                .item-desc {
                    font-size: 16px;
                    line-height: 22px;
                    font-weight: 500;
                    letter-spacing: -.02em;
                    opacity: .6;
                }
                &:not(:last-child)::after {
                    content: "";
                    position: absolute;
                    right: -40px;
                    top: 0;
                    height: 100%;
                    border-left: 1px solid #4B4B4B;
                    opacity: .6;
                }
            }
        }
    }
    .home-p4 {
        padding: 100px 40px;
        text-align: center;
        h3, .subtitle {
            margin-inline: auto;
        }
        h3 {
            max-width: 700px;
        }
        .subtitle {
            display: flex;
            justify-content: center;
            margin-bottom: 60px;
            span {
                display: flex;
                hr {
                    height: calc(100% - 10px);
                    margin: 5px 10px;
                    border-block: none;
                    //border-inline: 1px solid var(--color-Border);
                    border-inline: 1px solid #ffffff99;
                }
            }
        }
    }
    .home-p5 {
        padding: 100px 40px;
        text-align: center;
        .image-wrap {
            position: relative;
            display: flex;
            justify-content: center;
            height: 448px;
            overflow: hidden;
            margin-top: 60px;
            .image1, .image2 {
                padding: 16px;
                border-radius: 40px;
                border: 1px solid var(--color-BTN-primary);
                //background: var(--color-BG);
                background: #000000;
            }
            .image1 {
                width: 828px;
                height: 703px;
            }
            .image2 {
                margin: 64px 0 0 -32px;
                width: 302px;
                height: 661px;
            }
            img {
                width: 100%;
                height: auto;
                border-radius: 30px;
            }
            &::after {
                content: '';
                display: block;
                position: absolute;
                bottom: 0;
                width: 100%;
                height: 25%;
                //background: linear-gradient(#0000 0%, var(--color-BG) 100%);
                background: linear-gradient(#0000 0%, #000000 100%);
                z-index: 2;
            }
        }
    }
    .home-p6 {
        padding: 100px 40px;
        h3 {
            margin-bottom: 60px;
        }
        .faq-lists{
            display: flex;
            flex-direction: column;
            gap: 30px;
            .faq-item {
                width: 100%;
                height: min-content;
                display: flex;
                flex-direction: column;
                overflow: hidden;
                padding-bottom: 34px;
                //border-bottom: 1px solid var(--color-Border);
                border-bottom: 1px solid #ffffff99;
                gap: 20px;
            }
            .faq-title {
                display: flex;
                justify-content: space-between;
                width: 100%;
                height: 34px;
                display: flex;
                font-size: 20px;
                font-weight: 700;
                line-height: 34px;
                //color: var(--color-text-primary);
                color: #ffffff;
                cursor: pointer;
                &::after {
                    position: relative;
                    content: '';
                    display: block;
                    width: 10px;
                    min-width: 10px;
                    height: 10px;
                    min-height: 10px;
                    top: 6px;
                    //border: 2px solid var(--color-text-primary);
                    border: 2px solid rgb(218,218,218);
                    border-top: none;
                    border-right: none;
                    transform: rotate(-45deg);
                    margin-right: 8px;
                    transition: all .2s ease;
                    transform-origin: center;
                }
            }
            .faq-item.expand {
                //border-color: var(--color-text-primary);
                border-color: #ffffff;
                .faq-title::after {
                    transform: rotate(-225deg);
                    margin-bottom: -5px;
                    top: 12px;
                    border-color: rgb(255,255,255);
                }
            }
            .option {
                font-size: 18px;
                line-height: 27px;
                font-weight: 300;
            }
        }
    }
}
.home-container .content-w {
    max-width: 1100px;
    margin-inline: auto;
}
.home-container h3 {
    font-size: 48px;
    line-height: 54px;
    font-weight: 800;
    margin-bottom: 15px;
}
.home-container .subtitle {
    font-size: 18px;
    line-height: 27px;
    font-weight: 500;
    letter-spacing: -.02em;
    opacity: .6;
}
.isH5 .home-container h3 {
    font-size: 36px;
    line-height: 40px;
}
.isH5 .home-container .subtitle {
    font-size: 16px;
    line-height: 24px;
}

@media (min-width: 810px) and (max-width: 1439.98px) {
    .home-container {
        .home-p1 {
            padding: 100px 40px 60px;
            ul {
                gap: 12px;
                li {
                    flex: 1;
                    max-width: 266px;
                }
            }
        }
        .home-p2 {
            padding: 100px 40px;
            ul {
                margin-top: 40px;
                gap: 20px;
                li {
                    flex: 1;
                    min-height: 300px;
                    padding: 20px;
                    .logo {
                        width: 83px;
                        height: 100px;
                    }
                }
            }
        }
        .home-p3 {
            padding-block: 100px 68px;
            .video-wrap {
                width: 800px;
            }
            ul {
                padding-block: 32px;
                gap: 56px;
                li:not(:last-child)::after {
                    right: -28px;
                }
            }
        }
        .home-p5 {
            padding: 100px 40px;
        }
        .home-p6 {
            padding: 100px 40px;
        }
    }
}
@media (max-width: 809.98px) {
    .content-w {
        max-width: 100%;
        margin-inline: 0;
    }
    .home-container {
        h3 {
            font-size: 36px;
            line-height: 40px;
        }
        .subtitle {
            font-size: 16px;
            line-height: 24px;
        }
        .home-p1 {
            padding: 64px 20px;
            .p1-content {
                max-width: 100%;
            }
            ul {
                flex-direction: column;
                padding-block: 0;
                gap: 70px; //100px;
                align-items: center;
                li {
                    width: min-content;
                    height: min-content;
                    padding-block: 0;
                    border: none;
                    gap: 0; //36px;
                    .logos {
                        width: 198px;
                        height: auto;
                        .active {
                            display: none;
                        }
                        &:hover {
                            .normal {
                                display: block;
                            }
                            .active {
                                display: none;
                            }
                        }
                    }
                    img {
                        width: 100%;
                        height: 100%;
                    }
                    .item-name {
                        font-size: 28px;
                        line-height: 34px;
                    }
                    .item-desc {
                        font-size: 16px;
                        line-height: 24px;
                    }
                }
            }
        }
        .home-p2 {
            padding: 64px 24px;
            .top {
                display: block;
                .common-button {
                    width: 100%;
                    margin-top: 25px !important;
                }
            }
            ul {
                flex-direction: column;
                gap: 20px;
                li {
                    display: flex;
                    width: 100%;
                    min-height: auto;
                    padding: 24px;
                    gap: 20px;
                    .logo {
                        width: 73px;
                        min-width: 73px;
                        height: auto;
                        margin-bottom: 0;
                        img {
                            height: auto;
                        }
                    }
                }
            }
        }
        .home-p3 {
            padding: 64px 24px;
            overflow: hidden;
            .video-wrap {
                width: 100vw;
                margin-left: -30px;
            }
            ul {
                flex-direction: column;
                gap: 24px;
                li {
                    &:not(:last-child)::after {
                        right: 0;
                        top: auto;
                        bottom: -12px;
                        width: 100%;
                        height: 0;
                        border-left: none;
                        //border-top: 1px solid var(--color-Border);
                        border-top: 1px solid #ffffff99;
                    }
                }
            }
        }
        .home-p4 {
            padding: 64px 35px;
            .subtitle {
                flex-wrap: wrap;
                span {
                    display: flex;
                    hr {
                        height: calc(100% - 10px);
                        margin: 5px 10px;
                        border-block: none;
                        //border-inline: 1px solid var(--color-Border);
                        border-inline: 1px solid #ffffff99;
                    }
                }
            }
        }
        .home-p5 {
            padding: 64px 24px;
            .image-wrap {
                position: relative;
                justify-content: flex-start;
                height: 269px;
                margin-bottom: 10px;
                .image1, .image2 {
                    padding: 4px;
                    border-radius: 16px;
                }
                .image1 {
                    width: 408px;
                    min-width: 408px;
                    height: 231px;
                    margin-top: 36px;
                }
                .image2 {
                    position: absolute;
                    top: 0;
                    right: 0;
                    width: 122px;
                    min-width: 122px;
                    height: 268px;
                    padding: 6px;
                    margin: 0 0 0 -32px;
                    box-sizing: border-box;
                }
                img {
                    border-radius: 10px;
                }
                &::after {
                    content: '';
                    display: block;
                    position: absolute;
                    bottom: 0;
                    width: 100%;
                    height: 25%;
                    //background: linear-gradient(#0000 0%, var(--color-BG) 100%);
                    background: linear-gradient(#0000 0%, #000000 100%);
                    z-index: 2;
                }
            }
        }
        .home-p6 {
            padding: 64px 24px;
            .faq-lists{
                gap: 30px;
                .faq-item {
                    //min-height: 86px;
                }
                .faq-title {
                    height: min-content;
                    &::after {
                        top: 8px;
                    }
                }
                .faq-item.expand {
                    .faq-title::after {
                        top: 14px;
                    }
                }
            }
        }
    }
}


@keyframes drop-in {
  0% {
    height: 0;
    opacity: 0;
  }
  100% {
    height: min-content;
    opacity: 1;
  }
}

.faqbox-fade-enter-active {
  transform-origin: center top;
  animation: drop-in 0.2s ease;
}
.faqbox-fade-leave-active {
  transform-origin: center top;
  animation: drop-in 0.2s reverse ease;
}