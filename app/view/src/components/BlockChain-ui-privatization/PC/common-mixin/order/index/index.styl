.order-main{
  // 改，2022.10.16
  // width:1180px;
  width: 100%;
  min-height: 800px;
  margin: 0 auto;
  padding-left: 280px;
  box-sizing: border-box;
  position: relative;
}
.order-title {
  height: 56px;
  line-height: 56px;
  padding-left: 20px;
  font-size: 16px;
}
.margin-right{
  // width:980px;
  // 改，2022.10.16
  width: 100%
  min-height: 800px;
}
.margin-left{
  width: 280px;
  position: absolute;
  height: 100%;
  left: 0;
}
.Int {
  .order-page-box {
    height: 100%;
  }
  .order-main{
    margin: 0;
    width: auto;
    height: 100%;
    padding-left: 195px;
    .assets-page-title  {
        height: 80px;
        line-height: 80px;
        margin-bottom: 5px;
        .page-title-bar {
          width: 940px;
          margin: 0 auto;
          font-size: 16px;
        }
      }
    .margin-left {
      height: 100%;
    }
    .margin-right {
      width: auto;
      height: calc(100% - 85px);
      .margin-right-bar {
        width: 940px;
        margin: 0 auto;
        .otcOrder-options,
        .otcOrder-table,
        .otcOrder-nav,
        .exchangeOrder-table,
        .exchangeOrder-options,
        .exchangeOrder-nav {
          padding: 0;
        }
      }
    }
  }
}