.order-list-box {
    //margin-top: 5px;
    width: 100%;
    position: relative;
    .order-conent {
      width: 100%;
      position: relative;
      .common-table,
      .tab-box {
        min-width: 1200px;
      }
    }
    .notData {
      position: absolute;
      top: 71px;
      left: 0;
      right: 0;
      bottom: 0;
      padding-top: 80px;
      text-align: center;
      font-size: 12px;
      p {
        margin-top: 40px;
      }
      .icon {
        font-size: 36px;
      }
    }
  }
  .trade-content {
    .table-body .notShowe {
      display: none;
    }
    .table-body .marketPriceOrder {
     display: none;
    }
    .table-body {
      min-height: 300px;
    }
  }
  .tab-box {
    height: 44px;
    line-height: 44px;
    border-bottom-width: 1px;
    border-bottom-style: solid;
    padding: 0 15px;
    .tab-item {
      font-size: 14px;
      font-weight: 600;
      margin-right: 20px;
      display: inline-block;
      cursor: pointer;
      height: 37px;
       &:hover {
        color: var(--color-text-primary);
      }
      &.selected {
        border-bottom-style: solid;
        border-bottom-width: 2px;
      }
    }
    .all-order-btn {
      float: right;
      cursor: pointer;

    }
  }
  .opera {
    div {
      padding-right: 10px;
    }
  }
  // 国际版
  .Int-trade {
    .order-list-box {
      height: 400px;
      .order-conent {
        height: 350px;
      }
      .table-head {
        padding: 0 20px;
      }
    }
  }
